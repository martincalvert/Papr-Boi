!!! html
!!!
%html
  %head
    %title PaprBoi
    = stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = csrf_meta_tags
  %body
    - if session[:user].present?
      .navbar.navbar-inverse
        .navbar-header
          %button.navbar-toggle{:type => "button", :data => {:toggle => "collapse", :target => ".navbar-inverse-collapse"}}
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.navbar-brand{:href => "javascript:void(0)"} Brand
        .navbar-collapse.collapse.navbar-inverse-collapse
          %ul.nav.navbar-nav
            %li
              %a{:href => '#'} YAS
          %form.navbar-form.navbar-left
            %input.form-control.col-lg-8{:placeholder => "Search", :type => "text" }
          %ul.nav.navbar-nav.navbar-right
            %li
              %a{:href => "javascript:void(0)"} Link
            %li.dropdown
              %a.dropdown-toggle{:href => "#", :data => {:target => "#", :toggle => "dropdown"}} User
              %ul.dropdown-menu
                %li
                  %a Poop
                %li.divider
                %li
                  %a{:href => "javascript:void(0)"} Separated link
    .col-xs-12
      = yield
